{
  "name": "reporting-service",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js"
  },
  "devDependencies": {
    "eslint": "^5.15.1",
    "eslint-config-google": "^0.12.0"
  },
  "dependencies": {
    "bluebird": "^3.5.4",
    "cheerio": "^1.0.0-rc.3",
    "cron": "^1.7.2",
    "fastify": "^2.2.0",
    "fastify-cors": "^2.1.2",
    "fastify-env": "^0.6.2",
    "fastify-formbody": "^3.1.0",
    "request": "^2.88.0",
    "split2": "^3.1.1"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended",
      "google"
    ],
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "rules": {
      "camelcase": [
        1,
        {
          "properties": "never"
        }
      ],
      "max-len": [
        1,
        100,
        2
      ],
      "no-console": 0,
      "object-curly-spacing": [
        1,
        "always"
      ],
      "new-cap": 0
    }
  }
}
